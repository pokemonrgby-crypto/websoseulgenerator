{
  "regions": ["icn1"],
  "functions": {
    "api/generate.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "/api/generate", "dest": "/api/generate.js" },
    { "src": "/public/(.*)", "dest": "/public/$1" },
    { "src": "/novel.html", "dest": "/novel.html" },
    { "src": "/editor.html", "dest": "/editor.html" },
    { "src": "/", "dest": "/index.html" }
  ]
}
